{
	"[csharp]": {
		"editor.defaultFormatter": "csharpier.csharpier-vscode"
	},
	"[javascript]": {
		"editor.defaultFormatter": "vscode.typescript-language-features"
	},
	"[rust]": {
		"editor.defaultFormatter": "rust-lang.rust-analyzer"
	},
	"auto-close-tag.activationOnLanguage": [
		"HTML (EEx)",
		"HTML (Eex)",
		"blade",
		"cfml",
		"ejs",
		"erb",
		"javascriptreact",
		"jinja",
		"lang-cfml",
		"liquid",
		"markdown",
		"php",
		"plaintext",
		"plist",
		"typescript",
		"typescriptreact",
		"vue",
		"xml"
	],
	"auto-close-tag.disableOnLanguage": [],
	"cSpell.diagnosticLevel": "Hint",
	"cSpell.userWords": ["distro", "Hengeveld", "Newtonsoft", "TRPC"],
	"editor.accessibilitySupport": "off",
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.detectIndentation": false,
	"editor.fontFamily": "'JetBrainsMono NF', Consolas, 'Courier New', monospace",
	"editor.fontLigatures": false,
	"editor.formatOnSave": true,
	"editor.indentSize": "tabSize",
	"editor.insertSpaces": false,
	"editor.lightbulb.enabled": "off",
	"editor.linkedEditing": true,
	"editor.stickyScroll.enabled": false,
	"explorer.autoReveal": false,
	"explorer.compactFolders": false,
	"files.associations": {
		"*.module": "php"
	},
	"files.eol": "\n",
	"files.trimTrailingWhitespace": true,
	"git.autofetch": true,
	"git.confirmSync": false,
	"githubPullRequests.pullBranch": "never",
	"javascript.format.placeOpenBraceOnNewLineForControlBlocks": true,
	"php.inlayHints.parameters.byReference": false,
	"php.inlayHints.parameters.enabled": false,
	"php.inlayHints.types.lambdaParameter": false,
	"php.inlayHints.types.return": false,
	"prettier.tabWidth": 4,
	"prettier.useTabs": true,
	"remote.autoForwardPortsSource": "hybrid",
	"rust-analyzer.inlayHints.chainingHints.enable": false,
	"rust-analyzer.inlayHints.closingBraceHints.enable": false,
	"rust-analyzer.inlayHints.genericParameterHints.const.enable": false,
	"rust-analyzer.inlayHints.parameterHints.enable": false,
	"rust-analyzer.inlayHints.typeHints.enable": false,
	"security.workspace.trust.untrustedFiles": "open",
	"svelte.enable-ts-plugin": true,
	"terminal.integrated.fontLigatures.enabled": true,
	"workbench.colorTheme": "Material Theme Ocean High Contrast",
	"workbench.iconTheme": "material-icon-theme",
	"workbench.list.openMode": "doubleClick",
	"workbench.productIconTheme": "bongocat",
	"workbench.secondarySideBar.defaultVisibility": "hidden",
	"workbench.startupEditor": "none",
	"workbench.tree.enableStickyScroll": false,
	"workbench.tree.indent": 12
}
